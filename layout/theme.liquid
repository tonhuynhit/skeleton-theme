<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->  
<head>
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /><![endif]-->

  <title>
  {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}" />

  <meta name="viewport" content="width=device-width, initial-scale=1" />
  
  {% include 'open-graph-tags' %}
  {% include 'twitter-card' %}

  {{ content_for_header }}

  {{ "bootstrap.min.css" | asset_url | stylesheet_tag }}
  {{ "animate.css" | asset_url | stylesheet_tag }}
  {{ "owl.carousel.min.css" | asset_url | stylesheet_tag }}
  {{ "slick.css" | asset_url | stylesheet_tag }}
  {{ "chosen.min.css" | asset_url | stylesheet_tag }}
  {{ "font-awesome.min.css" | asset_url | stylesheet_tag }}
  {{ "themify-icons.css" | asset_url | stylesheet_tag }}
  {{ "ionicons.min.css" | asset_url | stylesheet_tag }}
  {{ "jquery-ui.css" | asset_url | stylesheet_tag }}
  {{ "meanmenu.min.css" | asset_url | stylesheet_tag }}
  {{ "style.css" | asset_url | stylesheet_tag }}
  {{ "responsive.css" | asset_url | stylesheet_tag }}


  {% comment %} {{ 'style.scss.css' | asset_url | stylesheet_tag }}
  {{ 'social/social-icons.css' | global_asset_url | stylesheet_tag }}
  {{ '//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.0.3/css/font-awesome.css' | stylesheet_tag }}
  
  {{ '//fonts.googleapis.com/css?family=Source+Sans+Pro:400,600,700,400italic,600italic,700italic' | stylesheet_tag }} {% endcomment %}

  <!--[if lt IE 9]>
  {{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
  <![endif]-->

  {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
  {% if template contains 'customers' %}  
  {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}
  
  <!-- Additional Shopify helpers that will likely be added to the global shopify_common.js some day soon. -->
  {{ 'shopify_common.js'  | asset_url | script_tag }}

  {{ 'js/option_selection.js' | asset_url | script_tag }}

  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js' | script_tag }}
  <script>jQuery('html').removeClass('no-js').addClass('js');</script>

</head>

<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >

  {% comment %} <div class="container"> {% endcomment %}

    {% include 'header' %}

    <div id="main" role="main">
      {% comment %}}
        We have a template file to render.
        That's what content_for_layout is for.
      {% endcomment %}
      {{ content_for_layout }}
    </div>

    {% include 'footer' %}

    {% comment %}Shopify does not support Internet Explorer 6 nor 7. It does encourage themes to support IE8 until the end of 2014.{% endcomment %}
    <!--[if lt IE 8]>
    <p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">activate Google Chrome Frame</a> to improve your experience.</p>
    <![endif]-->
    
  {% comment %} </div> {% endcomment %}
  <!-- end of #container -->

  {{ 'shop.js'  | asset_url | script_tag }}
  <!-- all js here -->
  {{ "js/vendor/jquery-1.12.0.min.js" | asset_url | script_tag }}
  {{ "js/popper.js" | asset_url | script_tag }}
  {{ "js/bootstrap.min.js" | asset_url | script_tag }}
  {{ "js/imagesloaded.pkgd.min.js" | asset_url | script_tag }}
  {{ "js/isotope.pkgd.min.js" | asset_url | script_tag }}
  {{ "js/ajax-mail.js" | asset_url | script_tag }}
  {{ "js/owl.carousel.min.js" | asset_url | script_tag }}
  {{ "js/plugins.js" | asset_url | script_tag }}
  {{ "js/main.js" | asset_url | script_tag }}

</body>
</html>